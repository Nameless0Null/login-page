doctype html
html 
    head
        meta(charset='utf-8')
    body
        h1 Login Page
        form(action='/login' method='post')
            p
                input(type='text' name='email' placeholder='example@gmail.com')
            p
                input(type='password' name='password' placeholder='password')
            p
                button Log In
            p
                h3 signup:
                    a(href='http://localhost:3000/signup')= signup

